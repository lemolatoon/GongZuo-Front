{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "generators": {
      "v3.0": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/apiClient",
        "inputSpec": "https://raw.githubusercontent.com/lemolatoon/GongZuo-Infra/master/gongzuo.yaml",
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter": true
        }
      }
    }
  }
}
